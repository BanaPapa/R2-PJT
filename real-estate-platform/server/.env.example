# 서버 설정
NODE_ENV=development
PORT=3001

# 프론트엔드 URL
FRONTEND_URL=http://localhost:5173

# 스케줄러 설정
ENABLE_SCHEDULER=true

# 데이터베이스 (SQLite 사용, 필요시 PostgreSQL 등으로 변경)
DATABASE_URL="file:./kb_data.db"

# KB 사이트 설정
KB_STATS_URL="https://www.kbland.kr/webview.html#/main/statistics?channel=kbland&tab=0"

# 로그 레벨
LOG_LEVEL=info